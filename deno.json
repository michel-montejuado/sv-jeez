{
  "version": "0.0.0",
  "tasks": {
    "dev": "deno run -A --node-modules-dir npm:vite",
    "build": "deno run -A --node-modules-dir npm:vite build",
    "preview": "deno run -A --node-modules-dir npm:vite preview",
    "serve": "deno run --allow-net --allow-read jsr:@std/http@1/file-server dist/"
  },
  "imports": {
    "@/": "./src/",
    "@deno/vite-plugin": "npm:@deno/vite-plugin@^1.0.5",
    "@emotion/react": "npm:@emotion/react@^11.14.0",
    "@emotion/styled": "npm:@emotion/styled@^11.14.1",
    "@mui/icons-material": "npm:@mui/icons-material@^7.3.1",
    "@mui/material": "npm:@mui/material@^7.3.1",
    "@types/react": "npm:@types/react@^19.1.9",
    "@types/react-dom": "npm:@types/react-dom@^19.1.7",
    "@vitejs/plugin-react-swc": "npm:@vitejs/plugin-react-swc@^3.10.2",
    "i18next": "npm:i18next@^25.3.4",
    "i18next-browser-languagedetector": "npm:i18next-browser-languagedetector@^8.2.0",
    "i18next-http-backend": "npm:i18next-http-backend@^3.0.2",
    "react": "npm:react@^19.1.1",
    "react-dom": "npm:react-dom@^19.1.1",
    "react-i18next": "npm:react-i18next@^15.6.1",
    "react-router": "npm:react-router@^7.8.0",
    "vite": "npm:vite@^7.0.6"
  },
  "compilerOptions": {
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "jsxImportSourceTypes": "@types/react"
  },
  "nodeModulesDir": "auto"
}